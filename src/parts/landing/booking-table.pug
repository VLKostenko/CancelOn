.booking-table
   span.search-filter-block
      .input-group.input-group-white.date-group
         span.input-group-addon
            i.fa.fa-calendar
         span.form-control.datepicker-input
            span.date May, 6th
            span.day Saturday
         span.form-control.datepicker-output
            span.date Jun, 14th
            span.day Wednesday
      .input-group.input-group-white.people-count
         span.input-group-addon
            i.fa.fa-sliders(aria-hidden='true')
         a#peopleCount.dropdown-toggle(href='javascript:void(0)', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false')
            .block
               span.value#rooms 3
               span.col-form-label Rooms
            .block
               span.value#adults 4
               span.col-form-label Adults
            .block
               span.value#children 0
               span.col-form-label Children
         span.down-arrow
            i.fa.fa-caret-down(aria-hidden='true')
         .dropdown-menu.book-menu(aria-labelledby='peopleCount')
            label.col-form-label.mt-20(for='quant[1]') Rooms
            .input-group.mt-10
               span.input-group-btn
                  button.btn.btn-info.btn-number(type='button', data-type='minus', data-field='quant[1]')
                     | -
               input.form-control.input-number(type='text', id='quant[1]', name='quant[1]', value='3', min='1', max='10', data-id='rooms')
               span.input-group-btn
                  button.btn.btn-warning.btn-number(type='button', data-type='plus', data-field='quant[1]')
                     | +
            label.col-form-label.mt-20(for='quant[2]') Adults
